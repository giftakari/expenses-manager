    "type": "git",
    "url": "git+https://github.com/tweneboah/expenses-manager.git"
  },
  "keywords": [
    "Expenses",
    "Income",
    "Resources"
  ],
  "author": "Emmanuel Tweneboah",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/tweneboah/expenses-manager/issues"
  },
  "homepage": "https://github.com/tweneboah/expenses-manager#readme",
  "dependencies": {
    "body-parser": "^1.19.0",
    "concurrently": "^5.0.0",
    "cors": "^2.8.5",
    "env": "^0.0.2",
    "express": "^4.17.1",
    "mongoose": "^5.7.6"
  }
}
